  "private": true,
  "dependencies": {
    "@azure/arm-monitor": "^7.0.0",
    "@azure/identity": "^4.5.0",
    "@azure/monitor-query": "^1.3.1",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/material": "^6.1.8",
    "@mui/x-data-grid": "^7.22.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.119",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "apexcharts": "^3.54.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "js-yaml": "^4.1.0",
    "react": "^18.3.1",
    "react-apexcharts": "^1.5.0",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-scripts": "5.0.1",
    "rehype-raw": "^7.0.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yaml-loader": "^0.8.1"
  },
  "proxy": "http://localhost:8000",
  "scripts": {
    "start-backend": "nodemon --exec \"ts-node ./src/data-server/server.ts\" --watch \"/src/data-server/**\" --ext \"ts\"",
    "start-frontend": "react-app-rewired start",
    "start": "npm-run-all --parallel start-backend start-frontend",
    "build": "ts-node ./src/dashboard-exporter/exporter.ts",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "import": "ts-node ./src/dashboard-importer/importer.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "chokidar": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.2.1"
  }
}
